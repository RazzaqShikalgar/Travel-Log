        <div>
          <h3>Special Attraction</h3>
          <p class="description">
            Things we can do visit whenever we come to that location
          </p>
          <p class="view-all-cards">Read More</p>
        </div>
        <div id="containerz">
          <div id="slider-container">
            <span onclick="slideRight()" class="btn"></span>
              <div id="slider">
                <% if(attraction !== undefined ){ %>
                  <% attraction.forEach(function(attraction, index){ %>
                <div class="slide">
                  <% if (attraction.photo !== undefined) {%>
                 <img src="<%=attraction.photo.images.original.url%>" alt="Image description">
                 <% } else{ %>
                  <a href="/location/detail">  <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/a/ac/No_image_available.svg/300px-No_image_available.svg.png" alt="George V"/></a>
                  <% } %>
                  <h2><%=attraction.name%></h2>
                   <!-- <div class="favorite"></div>  -->
                 <div class="carrousel-reviews">
                    <i class="icon-bubble_rating_full_refresh"></i>
                    <i class="icon-bubble_rating_full_refresh"></i>
                    <i class="icon-bubble_rating_full_refresh"></i>
                    <i class="icon-bubble_rating_full_refresh"></i>
                    <i class="icon-bubble_rating_full_refresh"></i>
                    <p><%=attraction.num_reviews%> Reviews</p>
                  </div>
                </div>
                <% }); %>
                <% } %>
              </div>
              <span onclick="slideLeft()" class="btn"></span>
            </div>
        </div>
        
      </div>


      <!-- Hotels Section -->
      <div class="carrousel">
        <div>
          <h3>Hotels</h3>
          <p class="description">
            Things we can do visit whenever we come to that location
          </p>
