</head>
<body class="body">
  <div id="loader">
    <div class="spinner"></div>
  </div>
  
  <!-- ---------- HEADER ---------- -->
  <header>
    <div class="container">
      <div>
        <i class="icon-menu_bars"></i>
        <img src="/images/logonew.JPG" alt="description" />
        <i class="icon-search"></i>
      </div>
      <nav>
        <div>
          <i class="icon-plus_c"></i>
          <p>Publish</p>
        </div>
        <div>
          <i class="icon-notification_bell"></i>
          <p>Alerts</p>
        </div>
        <div>
          <i class="icon-heart"></i>
          <p>Travel</p>
        </div>
        <button id="openForm">Connect-us</button>
      </nav>
    </div>
  </header>

  <!-- ---------- MAIN ---------- -->
  <main>
    <div class="container">
      <section>
        <h1><span class="title">Explore </span><%=place%></h1>
        <nav>
          <div>
            <p>Destination</p>
            <i class="icon-destination"></i>
          </div>
          
          <div>
            <p> vacation Location</p>
            <i class="icon-vacation_rentals"></i>
          </div>
          <div>
            <p>Activites</p>
            <i class="icon-attractions"></i>
          </div>
          <div>
            <p>Restaurants</p>
            <i class="icon-restaurants"></i>
          </div>
          <div>
            <p>Travel Forums </p>
            <i class="icon-forums"></i>
          </div>
          
         
        </nav>
      </section>
      <section class="pictures">
        <div>
        
          <img class="left-image" src="<%=location.result_object.photo.images.original.url%>" />
     
          <a href=""><i class="icon-photo"></i>All the Photos</a>
        </div>
        <div>
          
          <% if(freeimages !== undefined ){ %>
            <% freeimages.some(function(freeimages, index){ %>
            <% if(index !== 2) {%> 
                <img class="right-image-1" src="<%=freeimages.image%>" alt="description" />
                <% } else{ %>
                  <% return true; } %>
            <% }); %>
            <% } %>
        </div>
      </section>
      <nav class="menu">
        <p>
          <!-- <span>Europe</span> > <span>France</span> >
          <span>île-de-France</span> > -->
          <span><%=location.result_object.name%></span>
          <i class="icon-caret_down"></i>
        </p>
        <p>Visiter : <%=location.result_object.location_string%> </p>
      </nav>

      <section class="description">
        <div>
          <h2>The City of Light dazzles in every way</h2>
          <p>
          <%=location.result_object.description%>
          </p>
        </div>
        <div>
          <div>
            <h2>Start Planning <%=location.result_object.name%></h2>
            <p>
              Create a trip to save and organize all your ideas and visualize them on a map
            </p>
            <form action="/planner" method="POST">
            <div class="input-group rounded">
              <input type="text" name="days" class="form-control rounded" placeholder="No Of Days" aria-label="Search" aria-describedby="search-addon" />
              <span class="input-group-text border-0" id="search-addon">
                <i class="fas fa-search"></i>
              </span>
            </div>
            <div class="input-group rounded">
              <input type="text" name="location" class="form-control rounded" placeholder="Location" aria-label="Search" aria-describedby="search-addon" />
              <span class="input-group-text border-0" id="search-addon">
                <i class="fas fa-search"></i>
              </span>
            </div>
            <a href="/planner" target="_blank"><button type="submit">Submit</button></a>
          </form>
          </div>
          <img src="/assets/img/card.png" alt="description" />
        </div>
      </section>
    </div>
  </main>

  <!-- ---------- SECTION 1 ---------- -->

  <section class="section-1">
    <div class="section-title">
      <div class="border-section-left"></div>
      <h2>Travelling Tips</h2>
      <div class="border-section-right"></div>
    </div>

    <p class="view-all">Read More</p>

    <div class="advice-cards">
      <div>
        <img src="/assets/img/icone-1.svg" alt="icone 1" />
        <p>Best time to visit</p>
      </div>
      <div>
        <img src="/assets/img/icone-2.svg" alt="icone 2" />
        <p>Move</p>
      </div>
      <div>
        <img src="/assets/img/icone-3.svg" alt="icone 3" />
        <p>Local Customes</p>
      </div>
      <div>
        <img src="/assets/img/icone-4.svg" alt="icone 4" />
        <p>Tips from the pro's</p>
      </div>
    </div>
  </section>

  <!-- ---------- SECTION 2 ---------- -->

