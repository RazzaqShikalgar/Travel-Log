  <section>
    <div class="sec2">
      <div class=" container my-3 mt-5" id="featureContainer">
        <div class="row mx-auto my-auto justify-content-center">
          <div id="featureCarousel" class="carousel slide" data-bs-ride="carousel">
            <!-- Carousel Controls. OPTIONAL -->
            <div class="float-end pe-md-4">
              <a class="indicator" href="#featureCarousel" role="button" data-bs-slide="prev">
                <!-- <i class="fa-solid fa-arrow-right"></i>
                 -->
                <i class="fa fa-arrow-left fa-solid"></i>
              </a> &nbsp;&nbsp;
              <a class="w-auto indicator" href="#featureCarousel" role="button" data-bs-slide="next">
                <i class="fa fa-arrow-right"></i> </a>
            </div>
            <!-- Add Slides To The Carousel -->
            <div class="carousel-inner font" role="listbox">
              <h1>Where to Go Right Now</h1>
              <span class="font">Spots at the top of travellers’ must-go lists</span>
              <div class="carousel-item active">
                <div class="col-md-3">
                  <div class="card">
                    <div class="card-img">
                      <a href="/advisor/id"><img style="cursor:pointer" src="/uploads/card1.jpg" class="img-fluid"></a>
                    </div>
                    <div class="card-img-overlays">Taiwan</div>
                  </div>
                </div>
              </div>
              <% if(categories !='' ){ %>
                <% categories.forEach(function(Category, index){ %>
                  <div class="carousel-item">
                    <div class="col-md-3">
                      <div class="card">
                        <div class="card-img">
                          <a href="/advisor/<%=Category._id%>"><img src="<%=Category.image%>" alt="<%= Category.namee%>" target="_blank" class="img-fluid"></a>
                        </div>
                        <div class="card-img-overlays">
                          <%=Category.namee%>
                        </div>
                      </div>

                    </div>
                  </div>
                  <% }); %>
                    <% } %>
                      <!-- ... more slides here -->
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>


  <section class="gallery" id="gallery">

    <div class="heading font">
      <h1>Our Spacious gallery</h1>
      <span>we record memories</span>
    </div>

    <div class="box-container">
      <!-- <span id="more"> -->
      <%if(gallery !='' ){%>
        <%gallery.forEach(function(Gallery,index){%>
          <div class="box" data-aos="zoom-in-up" data-aos-delay="150">
            <img src="/uploads/<%=Gallery.image%>" alt="">
            <span class="material-symbols-outlined">
              north
            </span>
            <span>
              <%= Gallery.description %>
            </span>
            <h3>
              <%= Gallery.name %>
            </h3>
          </div>
          <%})%>
            <% } %>
              <!-- </span> -->
    </div>
    <button class="button-55" role="button" onclick="myFunction()" id="myBtn">Read More</button>

  </section>

