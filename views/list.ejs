  <!-- about section starts  -->

  <section class="about" id="about">

    <div class="video-container" data-aos="fade-right" data-aos-delay="300">
      <video src="images/about-vid-2.mp4" muted autoplay loop class="video"></video>
      <div class="controls">
        <span class="control-btn" data-src="images/about-vid-1.mp4"></span>
        <span class="control-btn" data-src="images/about-vid-2.mp4"></span>
        <span class="control-btn" data-src="images/about-vid-3.mp4"></span>
      </div>
    </div>

    <div class="content" data-aos="fade-left" data-aos-delay="600">
      <span  data-aos="fade-left" >why choose us?</span>
      <h3  data-aos="fade-left" >Choose Adventure. Choose Us.</h3>
      <p  data-aos="fade-right">Are you a fan of adventure and new experiences? Because our travel website is the gateway to a world full of them. So why settle for ordinary when you can choose extraordinary with us?</p>
      <a href="#" class="btn btns btn-outline-dark">Read more</a>
    </div>

  </section>


  <!-- destination section starts  -->

  <section class="destination" id="destination">

    <div class="headings font">
      <h1>our destination's</h1>
      <span>make yours destination</span>
    </div>

    <div class="box-container">
      <% if(cards !='' ) {%>
        <%cards.forEach(function(Cards, index){%>
          <div class="box" data-aos="fade-up" data-aos-delay="150">
            <div class="image">
              <img  data-aos="fade-up-right" src="/images/<%=Cards.image%>" alt="<%=Cards.name%>">
            </div>
            <div class="content">
              <h3  data-aos="fade-up-left" data-aos-delay="600">
                <%=Cards.name%>
                
              <p  data-aos="fade-left">
                <%=Cards.description%>
              </p>
              <a href="/details/<%=Cards.category%>"  data-aos="fade-up-left">Read more <i class="fas fa-angle-right"></i></a>
            </div>
          </div>
          <% }); %>
            <% } %>


    </div>
  </section>

  <!-- destination section ends -->
